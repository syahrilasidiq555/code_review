<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_config_settings_view_warehouse" model="ir.ui.view">
            <field name="name">res_config_settings_view_form_inherit_upscale_bill</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="stock.res_config_settings_view_form" />
            <field name="arch" type="xml">

                <xpath expr="//div[@name='warehouse_setting_container']" position="inside">
                    <div class="col-12 col-lg-6 o_setting_box" id="main_konsinyasi_warehouse" groups="base.group_no_one">
                        <div class="o_setting_right_pane">
                            <span class="o_form_label">Warehouse</span>
                            <div class="text-muted">
                                Set main and konsinyasi warehouse
                            </div>
                            <div class="row mt16">
                                <label for="main_warehouse_id" class="col-lg-4 o_light_label"/>
                                <field name="main_warehouse_id" class="oe_inline"/>
                            </div>
                            <div class="row mt16">
                                <label for="kons_warehouse_id" class="col-lg-4 o_light_label"/>
                                <field name="kons_warehouse_id" class="oe_inline"/>
                            </div>
                        </div>
                    </div>
                </xpath>

            </field>
        </record>
    </data>
</odoo>