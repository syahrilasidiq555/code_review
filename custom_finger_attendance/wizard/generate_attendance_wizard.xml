<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="generate_attendance_wizard_form_view" model="ir.ui.view">
            <field name="name">generate_attendance_wizard_form_view</field>
            <field name="model">generate.attendance.wizard</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Message">
                    <group name="head">
                        <field name="finger_config_id" invisible='1' />
                        <field name="work_time" />
                        <field name="attendance_date" />
                    </group>
                    <footer>
                        <button name="btn_continue"  type="object" string="Confirm" class="oe_highlight" confirm="Apakah anda yakin?"/>                             
                        <button special="cancel" string="Cancel"/>                    
                    </footer>                                    
                </form>
            </field>
        </record>
    </data>
</odoo>